@model DEV_dashboard_2019.Models.WidgetViewModel

@{
    ViewData["Title"] = "Widgets";
}

<p>
    <input type="button" value="AddWidget" class="btn btn-primary" onclick="window.location='@Url.Action("AddWidget", "Home")'"/>
</p>
<br />
<div class="row">
    @foreach (var widget in Model.WeatherConf)
    {
        <div class="col-md-6">
            @await Component.InvokeAsync("WeatherWidget", widget)
        </div>
    }
</div>